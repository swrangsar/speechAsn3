close all; clear all;

poleOrder = 6;
inputFile = 'a_pani.wav';
windowDuration = 0.030; % in ms

[y, fs] = preEmphasize(inputFile);

siz = size(y);
length = siz(1);
centralIndex = round(length/2);
M = round(windowDuration * fs);
startIndex = round(centralIndex - M/2);

windowedSignal = y(startIndex:startIndex + M-1);
autocorrelationCoefficients = zeros(poleOrder+1, 1);

for p = 1:poleOrder+1
    for k = 1:M
        valueToBeAdded = 0;
        if k-p > 0
            valueToBeAdded = y(k) * y(k-p);
            
        autocorrelationCoefficients[p] = autocorrelationCoefficients[p] + 
    

plot(windowedSignal);
